{
    "name": "paysera/lib-gedmo-translatable-integration-bundle",
    "description": "This library provides means to easily integrate Gedmo Translatable doctrine extension into your project.",
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.0",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/validator": "^3.0 || ^4.0",
        "symfony/security-core": "^3.0 || ^4.0",
        "symfony/routing": "^3.0 || ^4.0",
        "symfony/http-kernel": "^3.0 || ^4.0",
        "gedmo/doctrine-extensions": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "paysera/lib-php-cs-fixer-config": "^2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        }
    }
}
